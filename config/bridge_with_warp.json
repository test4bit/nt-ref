{
  "log": {
    "loglevel": "none"
  },

  "reverse": {
    "bridges": [
      {
        "tag": "bridge",
        "domain": "reverse.hellohabr.com"
      }
    ]
  },
  "outbounds": [
    {
      "protocol": "vless",
      "settings": {
        "vnext": [
          {
            "address": "__CLIENT_IP_PLACEHOLDER__",
            "port": 443,
            "users": [
              {
                "id": "__XRAY_UUID_PLACEHOLDER__",
                "encryption": "none"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "reality",
        "realitySettings": {
          "fingerprint": "chrome",
          "serverName": "cloudflare.com",
          "publicKey": "pNVkQojVsBa8lQZI9GKZOXJIyMdCSRYmhAJEbZcQlns",
          "spiderX": "",
          "shortId": ""
        }
      },
      "tag": "outgoing"
    },
    {
      "protocol": "wireguard",
      "settings": {
        "secretKey": "__WG_PRIVATE_KEY_PLACEHOLDER__",
        "address": ["__WG_ADDRESS_V4_PLACEHOLDER__"],
        "peers": [
          {
            "publicKey": "__WG_PEER_PUBLIC_KEY_PLACEHOLDER__",
            "endpoint": "__WG_PEER_ENDPOINT_PLACEHOLDER__",
            "keepAlive": 14
          }
        ]
      },
      "tag": "wireguard-1"
    },
    {
      "protocol": "freedom",
      "tag": "direct"
    },
    {
      "protocol": "blackhole",
      "tag": "block"
    }
  ],
  "routing": {
    "rules": [
      {
        "type": "field",
        "inboundTag": ["bridge"],
        "domain": ["full:reverse.hellohabr.com"],
        "outboundTag": "outgoing"
      },
      {
        "type": "field",
        "inboundTag": ["bridge"],
        "outboundTag": "wireguard-1"
      }
    ]
  }
}

